{
    "files.associations": {
        "*.ipynb": "jupyter",
        "*.ino": "cpp",
        "*.pl": "perl",
        "xiosbase": "c",
        "*.ipp": "c",
        "algorithm": "c",
        "list": "c",
        "memory": "c",
        "random": "c",
        "xhash": "c",
        "xlocale": "c",
        "xlocnum": "c",
        "xtree": "c",
        "xutility": "c",
        "sstream": "c",
        "strstream": "c",
        "xlocmon": "c"
    },
    "code-runner.executorMap": {
        "python": "python",
        "c": "cd $dir && gcc $fileName -l comdlg32 -o $fileNameWithoutExt && $dir$fileNameWithoutExt"
    }
    
}

